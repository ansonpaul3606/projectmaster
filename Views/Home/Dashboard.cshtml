
@model PerfectWebERP.Models.HomeModel.DashboardDetails
@{

    Layout = "~/Views/Shared/_Layout.cshtml";
}


<script src="~/Scripts/funnel/amcharts.js"></script>
<script src="~/Scripts/funnel/funnel.js"></script>
<script src="~/Scripts/pssChart.js"></script>
<link href="~/Assets/css/DashboardTable.css" rel="stylesheet" />
<style id="compiled-css" type="text/css">
    .easychart {
        position: relative;
        text-align: center;
    }
</style>
<style>
    .morris-hover {
        position: absolute;
        z-index: 1000;
    }

    .amcharts-funnel-tick {
        stroke-dasharray: 5;
    }

    .amcharts-chart-div svg + a {
        display: none !important;
    }

    .gridChart {
        max-height: 500px;
        /*height: 500px;*/
        overflow-y: auto;
    }

    #PanelChart20 th {
        white-space: nowrap;
        overflow: hidden;
    }



    .blurred-overlay {
        /* Initially hidden */
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        /* Semi-transparent background */
         /* Apply blur effect */
        z-index: 999; /* Layer it above the content but below the modal */
    }

    .modal {
        /*z-index: 1000;*/ /* Ensure the modal is above the overlay */
    }

</style>


<form id="" class="form-valide" perfect-class="form" action="#" method="post" perfect-onFormSuccess="">
    <div id="blurOverlay" class="blurred-overlay" style="display:block">
        <div class="modal " perfect-class="formGroupModalRemaining" id="formGroupModalRemaining" style="display:none">
            <div class="modal-dialog modal-dialog-centered " role="document">
                <div class="modal-content  rounded-xl">
                    <div class="modal-header border-0">
                        <h5 class="modal-title">Reminder <span class="fa fa-bell-o"></span></h5>
                        <button type="button" class="close" data-dismiss="modal" onclick="Closemodal(this)">
                            <span>&times;</span>
                        </button>
                    </div>
                    <div class="modal-body" perfect-class="formGroup" style="max-height: calc(100vh - 200px); overflow-y: auto;padding: 1rem;">
                        <div class="form-row">
                            <div class="col-sm-12" id="greatErequalTen">
                                <h5 style="text-align:center">Your subscription will expire in <b>@ViewBag.RemainingDays</b> days.<br /> Please renew your subscription to continue enjoying uninterrupted service.</h5>
                            </div>
                            

                        </div>
                        <div class="modal-footer" style="justify-content:center">
                            <button type="button" class="btn btn-primary1 align-content-center" data-dismiss="modal" perfect-css="button" onclick="Closemodal(this)" name="reset">OK</button>
                        </div>
                        <div class="col-md-12 mt-3" perfect-class="formGroupModalQuantitybody">


                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer border-0"></div>
        </div>
        <div class="modal " perfect-class="formGroupModalRemaining1" id="formGroupModalRemaining1" style="display:none">
            <div class="modal-dialog modal-dialog-centered " role="document">
                <div class="modal-content  rounded-xl">
                    <div class="modal-header border-0">
                        <h5 class="modal-title">Reminder <span class="fa fa-bell-o"></span></h5>
                        <button type="button" class="close" data-dismiss="modal" onclick="Closemodal(this)">
                            <span>&times;</span>
                        </button>
                    </div>
                    <div class="modal-body" perfect-class="formGroup" style="max-height: calc(100vh - 200px); overflow-y: auto;padding: 1rem;">
                        <div class="form-row">
                            <div class="col-sm-12" id="EqualZero">
                                <h5 style="text-align:center">Your subscription will expire <b>Today</b>.<br /> Please renew your subscription to continue enjoying uninterrupted service.</h5>
                            </div>


                        </div>
                        <div class="modal-footer" style="justify-content:center">
                            <button type="button" class="btn btn-primary1 align-content-center" data-dismiss="modal" perfect-css="button" onclick="Closemodal(this)" name="reset">OK</button>
                        </div>
                        <div class="col-md-12 mt-3" perfect-class="formGroupModalQuantitybody">


                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer border-0"></div>
        </div>
    </div>
    

    <div class="container-fluid">

        @Html.Raw((String)ViewBag.DashboardChartModal)
