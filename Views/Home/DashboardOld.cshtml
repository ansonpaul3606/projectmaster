        //    scales: {
        //        yAxes: [{
        //            ticks: {
        //                beginAtZero: true,
        //                min: 0,
        //                max: 100,
        //                stepSize: 20,
        //            }
        //        }]
        //    }
        //}
       function fn_InvoiceChart(element)
        {

         let $thisForm = $(element).closest('[perfect-class="form"]');

        let InputData={
        'month':document.getElementById('ddlInventoryMonth').value, //documen $thisForm.find('[name="ddlInventoryMonth"]').val(),
         'year': document.getElementById('ddlInventoryYear').value,//$thisForm.find('[name="ddlInventoryYear"]').val(),
         'mode':  document.getElementById('ddlInventoryMode').value//$thisForm.find('[name="ddlInventoryMode"]').val()
        };

          $.ajax({
               url: "@Url.Action("InventoryStatus", "Home")",
                data:
                {
                    "month": document.getElementById('ddlInventoryMonth').value,
                    "year":  document.getElementById('ddlInventoryYear').value,
                    "mode":  document.getElementById('ddlInventoryMode').value
                },
            type: "POST",
            dataType: "json",
            contentType: 'application/x-www-form-urlencoded;charset=utf-8',
             success: function (ajaxOutput) {
                  if (ajaxOutput.Process.IsProcess) {
                      let _data = []; let _labels = []; let _color = [];
                      $.each(ajaxOutput.objList, function (key, value) {
                          _data.push(value.Data);
                          _labels.push(value.Label);
                          _color.push(value.Color);
                      });

                   new Chart("InventoryChart", {
                       type: 'bar',
                       data: {
                           datasets: [{
                               data: _data,
                               borderWidth: 0,
                               backgroundColor: _color
                           }],
                           labels: _labels
                       },
                       options: {
                           responsive: true,
                           legend: false,
                           maintainAspectRatio: false
                       }
                   });
                 }
               }
        });
        }

    </script>


}

