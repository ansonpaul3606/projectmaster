@section scripts{

    <script>

       $(document).ready(function () {
           $('#jq-sidemenu').hide();
        fn_InvoiceChart(this);
       });


       function setSelectedValue(selectObj, valueToSet) {
           for (var i = 0; i < selectObj.options.length; i++) {
               if (selectObj.options[i].text == valueToSet) {
                   selectObj.options[i].selected = true;
                   return;
               }
           }
       }


         $.ajax({
               url: "@Url.Action("LeadStages", "Home")",
               type: "POST",
               dataType: "json",
               contentType: "application/json",
             success: function (ajaxOutput) {
                  if (ajaxOutput.Process.IsProcess) {
                       let _data = []; let _labels = [];
                   $.each(ajaxOutput.Data, function (key, value) {
                       _data.push(value.DashBoard);
                       _labels.push(value.Field);
                   });

                   new Chart("LeadStageChart", {
                       type: 'bar',
                       data: {
                           datasets: [{
                               data: _data,
                               borderWidth: 0,
                               backgroundColor: [
                                   "#ed5b5b",
                                   "#82bae9",
                                   "#a3c775"
                               ]

                           }],
                           labels: _labels
                       },
                       options: {
                           responsive: true,
                           legend: false,
                           maintainAspectRatio: false
                       }
                   });
                 }
               }
        });

         $.ajax({
               url: "@Url.Action("ServiceStages", "Home")",
               type: "POST",
               dataType: "json",
               contentType: "application/json",
             success: function (ajaxOutput) {
                  if (ajaxOutput.Process.IsProcess) {
                      let _data = []; let _labels = [];
                      $.each(ajaxOutput.Data, function (key, value) {
                          _data.push(value.DashBoard);
                          _labels.push(value.Field);
                   });

                      new Chart("ServiceStageChart", {
                          type: 'pie',
                       data: {
                           datasets: [{
                               data: _data,
                               borderWidth: 0,
                               backgroundColor: ["#9687f1", "#3ca2c8", "#d50000","#1bd084"]
                           }],
                           labels: _labels
                       }
                   });
                 }
               }
        });

         $.ajax({
               url: "@Url.Action("ProjectStatus", "Home")",
               type: "POST",
               dataType: "json",
               contentType: "application/json",
             success: function (ajaxOutput) {
                  if (ajaxOutput.Process.IsProcess) {
                      let _data = []; let _labels = []; let _color = [];
                      $.each(ajaxOutput.objList, function (key, value) {
                          _data.push(value.Data);
                          _labels.push(value.Label);
                          _color.push(value.Color);
                      });

                      new Chart("ProjectCompletedChart", {
                          type: 'bar',//'horizontalBar',
                       data: {
                           datasets: [{
                               data: _data,
                               borderWidth: 0,
                               backgroundColor: _color
                           }],
                           labels: _labels
                       },
                       options: {
                           responsive: true,
                           legend: false,
                           maintainAspectRatio: false,
                           scales: {
                               yAxes: [{
                                   ticks: {
                                       beginAtZero: true,
                                       min: 0,
                                       max: 100,
                                       stepSize: 20,
                                   }
                               }]
                           }
                       }
                   });
                 }
               }
        });
