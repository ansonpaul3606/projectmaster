@{
    ViewBag.Title = "";
}
<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12" id="jq_NotificationList_formContainer">
        </div>
    </div>
</div>



@section scripts{

    <script type="text/javascript">


        $(document).ready(function () {
            $('#jq-sidemenu').hide();
        });
        const $formContainer = $('#jq_NotificationList_formContainer');
        $(document).ready(function () {
            fn_NotificationList_InitialLoad();

        });


        function fn_NotificationList_InitialLoad() {
            $formContainer.load('@Url.Action("LoadNotificationList", "Notification")', function () {
                let $thisForm = $(this).find('[perfect-class="form"]');
                initFunctions($thisForm);
            });
        };

    </script>
}







