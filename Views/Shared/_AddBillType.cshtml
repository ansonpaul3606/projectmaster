            $(".DivSerAmtHead").hide();
            $(".DivSerAmtSubHead").hide();
            $(".DivBuyBackAmtHead").hide();
            $(".DivBuyBackAmtSubHead").hide();
            $(".nonRent1").hide();
            $(".DivFundHead").hide();

