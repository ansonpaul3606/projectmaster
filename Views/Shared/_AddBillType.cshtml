            $(".DivSerAmtSubHead").hide();
            $(".nonRent2").hide();
            $(".nonRent3").hide();
            $(".DivFundHead").hide();
        }
        else if ($thisForm.find('[name=BTBillType]').val() == 5){
            AcctHead.innerHTML = "Account Amount ";
            $(".nonRent").hide();
            $(".nonRent1").hide();
            AcctSubHead.innerHTML = "Account Sub ";
            $(".DivSecAmtHead").hide();
            $(".DivSerAmtHead").hide();
            $(".DivSerAmtSubHead").hide();
            $(".nonRent2").show();
