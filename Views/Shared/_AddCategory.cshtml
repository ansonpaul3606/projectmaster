    $(document).ready(function () {
        initFunctions('form');

        let ClientType = '@System.Configuration.ConfigurationManager.AppSettings["Lead"]';
        if (ClientType == "1") {
            $(".Prodlead").hide();
            $(".ProdProject").hide();
            $(".AllCat").show();
