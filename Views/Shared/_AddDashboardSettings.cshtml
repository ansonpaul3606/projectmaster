<script>
 //on load functions
    $(document).ready(function () {
        initFunctions('form');
        //this case is used for Subscription section. if clienttype value 1 , then it is a subscription client.
        let ClientType = '@System.Configuration.ConfigurationManager.AppSettings["Lead"]';
        if (ClientType == "1") {
            $(".NonLead").hide();
            $("#accordion-moduleCRM").hide();
            $("#accordion-moduleProject").hide();
            $("#accordion-moduleInventory").hide();
            $("#accordion-moduleAccounts").hide();
            $("#accordion-moduleProduction").hide();
            $("#accordion-modulePickUpDelivery").hide();
        }
        else {
            $(".NonLead").show();
            $("#accordion-moduleCRM").show();
            $("#accordion-moduleProject").show();
            $("#accordion-moduleInventory").show();
            $("#accordion-moduleAccounts").show();
            $("#accordion-moduleProduction").show();
            $("#accordion-modulePickUpDelivery").show();
        }
    });

  function fn_getUserlist(ele) {
        let $thisForm = $(ele).closest('[perfect-class="form"]');
      let $actionDropdown = $thisForm.find('[name="FK_User"]');
        $actionDropdown.empty();
        $actionDropdown.append($('<option/>', { value: '' }).text('Please select')).selectpicker('refresh');

        let inputData = {
            __RequestVerificationToken: $('[name="__RequestVerificationToken"]').val(),
            FK_UserRole: parseInt($thisForm.find('[name=FK_UserGroup]').val())
        };

        $.ajax({
            url: "@Url.Action("GetUserList", "DashboardSettings")",
            type: "POST",
            dataType: "json",
            data: inputData,
            contentType: 'application/x-www-form-urlencoded; charset=utf-8',
            success: function (ajaxOutput) {
                if (ajaxOutput.Process.IsProcess) {
                    $.each(ajaxOutput.Data, function (key, value) {
                        $actionDropdown.append($('<option/>', { value: value.UserID }).text(value.UserName)).selectpicker('refresh');
                    });
                    if ($actionDropdown.attr('perfect-onloadSelect')) {
                        $actionDropdown.val($actionDropdown.attr('perfect-onloadSelect')).selectpicker('refresh')
                    }
                }
            }
        });
    }

    function fn_updateDS(element,actionType)
    {
        debugger;
        let $thisForm = $(element).closest('[perfect-class="form"]');
        if ($thisForm.valid())
        {
            let $errorMsg = '';
            let $blnSuccess = false;
                    $(element).prop('disabled', true);


            var checkedModuleVals = $('.dsm:checkbox:checked').map(function () {
                return this.value;
            }).get();
            var checkedTileVals = $('.dsTile:checkbox:checked').map(function () {
                return this.value;
            }).get();
            var checkedChartVals = $('.dsChart:checkbox:checked').map(function () {
                return this.value;
            }).get();



            if (checkedModuleVals.length <= 0) {
                $blnSuccess = false;
                $errorMsg = 'Please select any modules';
            }
            else {
                $blnSuccess = true;
            }

            if (checkedTileVals.length <= 0 && $blnSuccess) {
                $blnSuccess = false;
                $errorMsg = 'Please select tile';
            }
            if (checkedChartVals.length <= 0 && $blnSuccess) {
                $blnSuccess = false;
                $errorMsg = 'Please select chart';
            }

            let _ID_DashboardSettings = 0;
                    let _ActionUrl = "";

                    if (actionType === "new") {
                        _ID_DashboardSettings = 0;
                        _ActionUrl = "@Url.Action("AddNewDashboardSettings", "DashboardSettings")";
                    }
                    else if (actionType === "update") {
                        _ID_DashboardSettings = $("#ID_DashboardSettings").val();
                        _ActionUrl = "@Url.Action("UpdateDashboardSettings", "DashboardSettings")";
                    }
                    var checkedTileMode = $('.dsTile:checkbox:checked').map(function () {
                        return $(this).closest('.card-footer').find('.dsModeTile').val();
                    }).get();
					
                    var checkedChartMode = $('.dsChart:checkbox:checked').map(function () {
                        return $(this).closest('.card-footer').find('.dsModeChart').val();
                    }).get();

                    let inputData = {
                        __RequestVerificationToken: $('[name="__RequestVerificationToken"]').val(),
                        'ID_DashboardSettings': _ID_DashboardSettings,
                        'EffectDate': $("#EffectDate").val(),
                        'FK_UserGroup': $("#FK_UserGroup").val(),
                        'FK_User': $("#FK_User").val(),
                        'FK_Mode':0,
                        'ModuleList': checkedModuleVals.join(","),
                        'TileList': checkedTileVals.join(","),
                        'ChartList': checkedChartVals.join(","),
                        'TileMode': checkedTileMode.join(","),
                        'ChartMode': checkedChartMode.join(",")
                    };

            if ($blnSuccess)
                            {
                               $.ajax({
                                         url: _ActionUrl,
                                         type: "POST",
                                         data: inputData,
                                         dataType: "json",
                                         contentType: 'application/x-www-form-urlencoded; charset=utf-8',
                                         success: function (ajaxOutput) {
                                             if (ajaxOutput.Process.IsProcess) {
                                                 $.each(ajaxOutput.Process.Message, function (key, value) {
                                                     NotificationMessage({ 'type': 'success', 'heading': 'Success', 'message': value });
                                                     $(element).prop('disabled', false);
                                                     DashboardSettingsInitialLoad();
                                                 });
                                             }
                                             else {
                                                 $.each(ajaxOutput.Process.Message, function (key, value) {
                                                     NotificationMessage({ 'type': 'error', 'heading': 'Error', 'message': value });
                                                     $(element).prop('disabled', false);
                                                     DashboardSettingsInitialLoad();
                                                 });
                                             }

                                             $thisForm.find('[perfect-ctype="select"]').val('').selectpicker('refresh');
                                             $thisForm.find('[perfect-ctype="input"]').val('');
                                             $thisForm.find('[perfect-ctype="date"]').val('');
                                             let onSuccess = $thisForm.attr('perfect-onFormSuccess')
                                             if (onSuccess) {
                                                 window[onSuccess]();

                                             }
                                         },
                                         complete: function ()
                                         {
                                                $(element).prop('disabled', false);
                                         }
                                    });
            }
            else
            {
                NotificationMessage({ 'type': 'error', 'heading': 'Error', 'message': $errorMsg });
                        $(element).prop('disabled', false);
            }
        }
    }
    function fn_resetDashboardSettings(ele)
    {
        DashboardSettingsInitialLoad();
    }



</script>



