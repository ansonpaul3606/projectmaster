<script>
    (function () {
        //jQuery('form').validate();//<-----  Validate/initialize our form on page load

    })();

    $(document).ready(function () {

        
        GetSecurityDetals("CMMN");
    });
    function GetSecurityDetals(SecModule) {
      



            $("#" + SecModule + "EffectDate").val(moment().format('YYYY-MM-DD'));
            var inputData = {
                __RequestVerificationToken: $('[name="__RequestVerificationToken"]').val(),
                'SecModule': SecModule,


            }


            $.ajax({
                url: "@Url.Action("GetGeneralModuleSettingsDetails", "GeneralModuleSettings")",
                type: "POST",
                data: inputData,
                dataType: "json",
                contentType: 'application/x-www-form-urlencoded; charset=utf-8',
                success: function (ajaxOutput) {
                    console.log(ajaxOutput);
                    if (ajaxOutput.Process.IsProcess) {
                        var MyData = ajaxOutput.Data;
                        console.log(ajaxOutput.Data);
                        console.log(ajaxOutput.Data.length, "------------length");
                        if (MyData.length > 0) {

                            var dte = moment(MyData[0].SecDate).format('DD/MM/YYYY');
                            $("#" + SecModule + "LastModifiedDate").text(" : Last Modified on " + dte);
                            $("#" + SecModule + "EffectDate").attr('min', moment().format('YYYY-MM-DD'));


                        }
                        var inputType = "";
                        var dataId = "";
                        $.each(MyData, function (i, val) {

                            $('.' + SecModule).each(function () {
                                inputType = $(this).attr("perfect-ctype");
                                dataId = $(this).attr("data-id");

                                var cntrl = $(this);
                                if (dataId.trim() == val.SecField.trim()) {
                                    switch (inputType) {
                                        case "select": $(this).val(val.SecValue);
                                            break;
                                        case "checkbox": if (val.SecValue.trim() == "0") {
                                            $(this).attr("checked", false);
                                        } else {
                                            $(this).attr("checked", true);
                                        }
                                            break;
                                    }
                                }
                            })
                        })

                    }
                }

            });
         
    }
    function ResetButton(ele) {
        GenModSet_initLoad();
    }

    function UpdateGenModeData(ele, actionType) {

        let GenModSettings = []; var SecValue = ""; var SecField = ""; var SecModule = actionType; var type = ""; var EffectDate = $('#' + actionType + "EffectDate").val();
        var IsValid = true;
        if (EffectDate == null) {

            IsValid = false; $('#' + actionType + "EffectDateErr").text('Please Enter Effect Date')
         }
        else if (EffectDate.trim() == "") {
            IsValid = false; $('#' + actionType + "EffectDateErr").text('Please Enter Effect Date')
        }
        else {
            $('.' + actionType).each(function () {

                type = $(this).attr("perfect-ctype"); console.log(type);
                SecField = $(this).attr("data-id");
                if (type == "select") {
                    SecValue = $(this).val();
                }
                else if (type == "checkbox") {
                    SecValue = $(this).is(":checked") == true ? "1" : "0";

                }
                let GenModSettingInfo = {};
                GenModSettingInfo['SecField'] = SecField;
                GenModSettingInfo['SecValue'] = SecValue;
                GenModSettings.push(GenModSettingInfo);

            })
            var inputData = {
                __RequestVerificationToken: $('[name="__RequestVerificationToken"]').val(),
                'EffectDate': EffectDate,
                'SecModule': SecModule,
                'SettingsDetails': GenModSettings,

            }

            if (GenModSettings.length > 0) {
                $.ajax({
                    url: "@Url.Action("SaveGeneralModuleSettings", "GeneralModuleSettings")",
                    type: "POST",
                    data: inputData,
                    dataType: "json",
                    contentType: 'application/x-www-form-urlencoded; charset=utf-8',
                    success: function (ajaxOutput) {

                        if (ajaxOutput.Process.IsProcess) {
                            $.each(ajaxOutput.Process.Message, function (key, value) {
                                NotificationMessage({ 'type': 'success', 'heading': 'Success', 'message': "Saved Successfully" });
                            });
                            GetSecurityDetals(actionType);
                        } else {

                            $.each(ajaxOutput.Process.Message, function (key, value) {

                                NotificationMessage({ 'type': 'error', 'heading': 'Error', 'message': value });
                            });

                        }
                    }
                });
            }
        }


    }

</script>