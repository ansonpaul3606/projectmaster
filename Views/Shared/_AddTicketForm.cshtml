 .timeline {
	 position: relative;
}
    .timeline::before {
        content: '';
        background: #b8c9cb;
        width: 5px;
        height: 95%;
        position: absolute;
        left: 50%;
        transform: translateX(-50%);
    }
 .timeline-item {
	 width: 100%;
	 margin-bottom: 70px;
}
