                                }

                                if (dtable[i].Mode == 3) {


                                    let div = document.createElement("div");
                                    div.className = "process-list col-12";

                                    let div1 = document.createElement("div");
                                   
                                        div1.className = "item3  w-180 blue-d-bg"
                                  
                                    let text = document.createTextNode(dtable[i].Status);
                                    let br1 = document.createElement('br');
                                    let text1 = document.createTextNode("on");
                                    let br2 = document.createElement('br');
                                    let text3 = document.createTextNode(dtable[i].TransDate);
                                    var span = document.createElement('span');
