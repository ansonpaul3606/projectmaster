	 content: '';
	 display: block;
	 clear: both;
}
 .timeline-content {
	 position: relative;
	 width: 45%;
	 padding: 10px 30px;
	 border-radius: 4px;
	 background: #f5f5f5;
	 box-shadow: 0 20px 25px 20px rgba(0, 0, 0, .3);
}
