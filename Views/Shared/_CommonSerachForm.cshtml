@model  List<PerfectWebERP.Models.CommonModel.PageDetails>


@helper CreateQuickNode(PerfectWebERP.Models.CommonModel.PageDetails Page)
{
                    /**/



    @*<a href="#">
            <div class="media-icon">
                <i class="fa fa-home"></i>
            </div>
        </a>*@
                    /**/



    <a href="@(Url.Action(@Page.Url, @Page.ControllerName)=="/"?"#":Url.Action(@Page.Url, @Page.ControllerName))">
        <span class="">

            <h4 class="fs-16 mb-1" style="color:crimson">@Page.MnuLstName</h4>
            <hr />
        </span>



    </a>

}

@if (Model.Count == 0)
{
    <h4 class="fs-16 mb-1" style="color:crimson">No Data Found</h4>
}
else
{

    foreach (var @sideData in Model)
    {


        @CreateQuickNode(sideData)
    }
}




