

@{
    ViewBag.Title = "Index";
}


<div class="container-fluid">
    @Html.AntiForgeryToken()
    <div class="row">
        <div class="col-lg-12" id="WalkingCustomerreport-form">

        </div>
    </div>
</div>

@section scripts {

    <script type="text/javascript">

        const $formContainer = $('#WalkingCustomerreport-form');
       

         $(document).ready(function () {

            fn_WalkingCustomerreport_initialLoad();


        });


        function fn_showFilter() {
            let $thisForm = $('[perfect-class="form"]');
            $thisForm.closest('.jq_leadgeneration_body').find('.leadformSesction').toggleClass('d-none');
        }

        function fn_WalkingCustomerreport_initialLoad() {
           
            LoadAddWalkingCustomerreport();

        }
        var validationMessage = {
            'datefilter': {
                'required': 'Please Select  Date Range'
            },
            'FK_Report': {
                'required': 'Please Select Report'
            }
        }

        function LoadAddWalkingCustomerreport() {

            
            $formContainer.load('@Url.Action("LoadWalkingcustomerlistReport", "WalkingCustomerReport")?mtd=@ViewBag.mtd' , function () {

                let $thisForm = $(this).find('[perfect-class="form"]');
                $thisForm.validate({ messages: validationMessage });
                $("#divWalkingList").show();
                $("#divWalkingLeadList").hide();
              //  $thisForm.closest('.jq_leadgeneration_body').find('.leadformSesction').toggleClass('d-none');
                $thisForm.attr('perfect-onFormSuccess', 'fn_WalkingCustomerreport_initialLoad');
                initFunctions($thisForm);
            });
        }
         function LoadAddWalkingCustomerreportrefresh() {

            
            $formContainer.load('@Url.Action("LoadWalkingcustomerlistReport", "WalkingCustomerReport")?mtd=@ViewBag.mtd' , function () {

                let $thisForm = $(this).find('[perfect-class="form"]');
                $thisForm.validate({ messages: validationMessage });
                $("#divWalkingList").show();
                $("#divWalkingLeadList").hide();
                $thisForm.closest('.jq_leadgeneration_body').find('.leadformSesction').toggleClass('d-none');
                $thisForm.attr('perfect-onFormSuccess', 'fn_WalkingCustomerreport_initialLoad');
                initFunctions($thisForm);
            });
        }
    </script>
}

